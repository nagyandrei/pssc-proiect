@model IterfataUtilizator.Models.MasinaMVC
@{
    ViewBag.Title = "View";
}
<h2>CautareMasina</h2>

@using (Html.BeginForm("CautaMasina", "Home", FormMethod.Post, new { @class = "linkStyle", mVC = @Model }))
{
    <div>
        <fieldset>
            <div class="editor-label">
                @Html.LabelFor(m => m.Marca)
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(m => m.Marca)
            </div>
            <div class="editor-label">
                @Html.LabelFor(m => m.An)
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(m => m.An)
            </div>
            <div class="editor-label">
                @Html.LabelFor(m => m.CIV)
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(m => m.CIV)
            </div>
            <input type="submit" value="Cautare" />
        </fieldset>
    </div>
}

